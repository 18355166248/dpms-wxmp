{
  "pages": [
    // pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "uni-app"
      }
    },
    {
      "path": "pages/appointmentDetail/appointmentDetail",
      "style": {
        "navigationBarTitleText": "预约详情"
      }
    },
    {
      "path": "pages/appointmenAmend/appointmenAmend",
      "style": {
        "navigationBarTitleText": "修改预约"
      }
    },
    {
      "path": "pages/personManagement/personManagement",
      "style": {
        "navigationBarTitleText": "人员管理"
      }
    },
    {
      "path": "pages/personAdd/personAdd",
      "style": {
        "navigationBarTitleText": "新增人员"
      }
    },
    {
      "path": "pages/personDetail/personDetail",
      "style": {
        "navigationBarTitleText": "人员详情"
      }
    },
    {
      "path": "pages/personAmend/personAmend",
      "style": {
        "navigationBarTitleText": "修改人员"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#5CBB89",
    "backgroundColor": "#F8F8F8",
    "usingComponents": {
      "van-cell-group": "./wxcomponents/vant-weapp/cell-group/index",
      "van-cell": "./wxcomponents/vant-weapp/cell/index"
    }
  },
  "lazyCodeLoading": "requiredComponents",
  "easycom": {
    "autoscan": true,
    "custom": {
      "^dpms(.*)": "@/dpmsComponents/dpms$1/dpms$1.vue"
    }
  }
}
